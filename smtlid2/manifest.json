{
  "name": "SMTLiD",
  "manifest_version": 2,
  "description": "Provides a toolbar with LeadiD to copy",
  "version": "0.7.2",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_title": "SMTLiD"
    },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
    },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["jquery-3.2.1.min.js", "content.js"],
      "css": ["style.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "toolbar.html",
    "style.css"
  ]
}
